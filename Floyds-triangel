public class PatternPY {
    public static void Floyds_triangel(int n) {
      //outer
        int i;
        int j;
        int counter = 1;
        for(i=1; i<=n; i++) {
            //inner
            for (j=1; j<=i; j++) {
                System.out.print(counter +" ");
                counter++;
            }
            System.out.println();
        }
    }

    public static void main(String[] args) {
        Floyds_triangel(5);
    }
}
